<html>
	<head>
		<style>
			.linecircle{
				width: 200px;
				height: 300px;
				background-color: #f54edc;
			}
			.circlerectangle{
				width: 200px;
				height: 300px;
				background-color: #fab948;
			}
			.polylinecircle{
				width: 200px;
				height: 300px;
				background-color: #6583f7;
			}
			.polygonellipse{
				width: 200px;
				height: 300px;
				background-color: #58f57f;
			}
		</style>
	</head>
	<body>
		<table>
		<th>
			<div class="linecircle"> 
				<svg style="position: absolute;" width="200" height="300">
				<line x1="0" y1="0" x2="200" y2="300" stroke-width="5" stroke="#fcba03"/>
				</svg>
				<svg width="320" height="50">
				<circle cx="100" cy="30" r="10" fill="#ed2005"/>
				<circle cx="130" cy="30" r="20" fill="#027d25"/>
				<circle cx="160" cy="30" r="10" fill="#fff424"/>
				</svg>
			</div>
		</th>
		<th>
			<div class="circlerectangle"> 
				<svg width="320" height="40">
				<circle cx="100" cy="30" r="10" fill="#ed2005"/>
				<circle cx="130" cy="30" r="10" fill="#027d25"/>
				<circle cx="160" cy="30" r="10" fill="#fff424"/>
				</svg>
				<svg width="320" height="150" style="margin: 10px;">
				   <rect width="180" height="50" fill="#d97400"/>
				</svg>
			</div>
		</th>
		<th>
			<div class="polylinecircle">
				<svg width="320" height="40">
				<circle cx="100" cy="30" r="10" fill="#ed2005"/>
				<circle cx="130" cy="30" r="10" fill="#027d25"/>
				<circle cx="160" cy="30" r="10" fill="#fff424"/>
				</svg>
				<svg height="50" style="margin-top: 20px;">
				   <polyline points="0,20 20,40 40,40 40,60 60,80 80,80 80,100 100,100 100,120 120,120 120,140 140" fill="none" stroke-width="5" stroke="#b1fcf9" transform="rotate(-45)"/>
				</svg>
				<svg height="50">
				   <polyline points="0,20 20,40 40,40 40,60 60,80 80,80 80,100 100,100 100,120 120,120 120,140 140" fill="none" stroke-width="2" stroke="#b1fcf9" transform="rotate(-45)"/>
				</svg>
			</div>
		</th>
		<th>
			<div class="polygonellipse"> 
				
				<svg width="300" height="90" >
				<ellipse cx="60" cy="30" rx="50" ry="25" fill="#027d25"/>
				<ellipse cx="145" cy="60" rx="50" ry="25" fill="#fff424"/>
				</svg>
				<svg height="250">
				<polygon points="100,5 125,95 80,125" style="fill:lime;stroke:purple;stroke-width:10" />
				</svg>
			</div>
		</th>
		<th>
			<div class="linecircle">
				<svg style="position: absolute;" width="200" height="300">
				<line x1="0" y1="0" x2="200" y2="300" stroke-width="3" stroke="#fcba03"/>
				</svg>
				<svg width="320" height="50">
				<circle cx="100" cy="30" r="10" fill="#ed2005"/>
				<circle cx="130" cy="30" r="20" fill="#027d25"/>
				<circle cx="160" cy="30" r="10" fill="#fff424"/>
				</svg>
				<svg width="320" height="150" style="margin: 10px;">
				   <rect width="180" height="50" fill="#d97400"/>
				</svg>
			</div>
		</th>
		<th>
			<div class="polylinecircle">
				<svg height="50" style="margin-top: 20px;">
				   <polyline points="0,20 20,40 40,40 40,60 60,80 80,80 80,100 100,100 100,120 120,120 120,140 140" fill="none" stroke-width="5" stroke="#b1fcf9" transform="rotate(-45)"/>
				</svg>
				<svg height="50">
				   <polyline points="0,20 20,40 40,40 40,60 60,80 80,80 80,100 100,100 100,120 120,120 120,140 140" fill="none" stroke-width="2" stroke="#b1fcf9" transform="rotate(-45)"/>
				</svg>
				<svg height="250">
				<polygon points="100,5 125,95 80,125" style="fill:purple;stroke:lime;stroke-width:10" />
				</svg>
			</div>
		</th>
		<th>
			<div class="circlerectangle">
			<svg style="position: absolute;" width="200" height="300">
				<line x1="0" y1="0" x2="200" y2="300" stroke-width="3" stroke="#fc5538"/>
				</svg>
				<svg width="300" height="50">
				<circle cx="100" cy="30" r="10" fill="#ed2005"/>
				<circle cx="130" cy="30" r="10" fill="#027d25"/>
				<circle cx="160" cy="30" r="10" fill="#fff424"/>
				</svg>
				<svg width="300" height="90" >
				<ellipse cx="60" cy="30" rx="50" ry="25" fill="#027d25"/>
				<ellipse cx="145" cy="60" rx="50" ry="25" fill="#fff424"/>
				</svg>
				<svg height="150" style= "position: absolute;">
				   <polyline points="0,20 20,40 40,40 40,60 60,80 80,80 80,100 100,100 100,120 120,120 120,140 140" fill="none" stroke-width="2" stroke="#b1fcf9" transform="rotate(-45)"/>
				</svg>
				<svg height="200">
				<polygon points="100,5 125,95 80,125" style="fill:lime;stroke:purple;stroke-width:1" />
				</svg>
			</div>
		</th>
	</body>
</html>
